Tenant Table	
TenantID	varchar(255)
UnitID	varchar(6)
RoomID	varchar(6)
BedID	varchar(6)
AgentID	varchar(6)
TenantName	varchar(255)
TenantPhoneNo	varchar(20)
TenantEmail	varchar(255)
RentStartDate	date
RentExpiryDate	date
TenantStatus	varchar(255)
CreatedAt	datetime
UpdatedAt	datetime
RentalType	varchar(255)
	
Payment Table	
PaymentID	varchar(50)
TenantID	varchar(255)
RoomID	varchar(255)
BedID	varchar(255)
AgentID	varchar(255)
DateCreated	datetime
Month	varchar(50)
Year	int(11)
Amount	decimal(10,2)
Overpayment	decimal(10,2)
PaymentType	varchar(50)
ChargeFee	decimal(10,2)
PaymentStatus	varchar(50)
Remarks	varchar(255)
Claimed	tinyint(1)
Claimer	varchar(255)
	
PaymentHistory Table	
PaymentHistoryID	varchar(50)
TenantID	varchar(255)
AgentID	varchar(6)
Amount	decimal(10,2)
PaymentDate	datetime
PaymentType	varchar(50)
Remarks	varchar(255)
	
Deposit Table	
DepositID	varchar(255)
TenantID	varchar(255)
RoomID	varchar(255)
BedID	varchar(6)
AgentID	varchar(6)
BedRentAmount	decimal(10,2)
DepositAmount	decimal(10,2)
PaidAmount	decimal(10,2)
RemainingAmount	decimal(10,2)
PaymentMadeDate	datetime
	
Bed  Table	
BedID	varchar(6)
RoomID	varchar(6)
UnitID	varchar(6)
BedNo	varchar(255)
BaseRentAmount	decimal(10,2)
BedStatus	varchar(50)
CreatedAt	datetime
UpdatedAt	datetime
AgentID	varchar(6)
BedRentAmount	decimal(10,2)
	
Room Table	
RoomID	varchar(6)
UnitID	varchar(6)
RoomNo	varchar(255)
RoomRentAmount	decimal(10,2)
Katil	int(11)
RoomStatus	varchar(50)
CreatedAt	datetime
UpdatedAt	datetime
AgentID	varchar(6)
